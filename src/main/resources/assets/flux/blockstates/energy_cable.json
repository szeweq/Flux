{
  "multipart": [
    {   "when": { "north": true },
      "apply": { "model": "flux:block/energy_cable_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "flux:block/energy_cable_side", "y": 90, "uvlock": true }
    },
    {   "when": { "south": true },
      "apply": { "model": "flux:block/energy_cable_side", "y": 180, "uvlock": true }
    },
    {   "when": { "west": true },
      "apply": { "model": "flux:block/energy_cable_side", "y": 270, "uvlock": true }
    },
    {   "when": { "up": true },
      "apply": { "model": "flux:block/energy_cable_side", "x": 270, "uvlock": true }
    },
    {   "when": { "down": true },
      "apply": { "model": "flux:block/energy_cable_side", "x": 90, "uvlock": true }
    },
    {   "when": { "north": false },
      "apply": { "model": "flux:block/energy_cable_noside", "weight": 2 }
    },
    {   "when": { "east": false },
      "apply": { "model": "flux:block/energy_cable_noside", "weight": 2, "y": 90, "uvlock": true }
    },
    {   "when": { "south": false },
      "apply": { "model": "flux:block/energy_cable_noside", "weight": 2, "y": 180, "uvlock": true }
    },
    {   "when": { "west": false },
      "apply": { "model": "flux:block/energy_cable_noside", "weight": 2, "y": 270, "uvlock": true }
    },
    {   "when": { "up": false },
      "apply": { "model": "flux:block/energy_cable_noside", "weight": 2, "x": 270, "uvlock": true }
    },
    {   "when": { "down": false },
      "apply": { "model": "flux:block/energy_cable_noside", "weight": 2, "x": 90, "uvlock": true }
    }
  ]
}
